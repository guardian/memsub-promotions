{
  "name": "MembershipSub-Promotions-PromoCode-View",
  "version": "0.0.1",
  "description": "A lambda to create a view of the Promotions and Campaigns table keyed by the promo code.",
  "buildDir": "dist",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.28.6"
  },
  "scripts": {
    "dist": "yarn install --production --modules-folder dist/node_modules",
    "compile": "babel src -d dist",
    "clean": "rm -rf dist && rm -rf target"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.980.0",
    "@aws-sdk/client-kms": "^3.985.0",
    "@aws-sdk/client-secrets-manager": "^3.981.0",
    "@aws-sdk/lib-dynamodb": "^3.962.0",
    "@aws-sdk/types": "^3.965.0"
  }
}
