{
  "name": "memsub-promotions",
  "author": "Memsubity team",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "echo 'Cleaning Public Folder' && rimraf ../public/javascripts && rimraf ../public/stylesheets",
    "test": "echo \"Error: no test specified\" && exit 1",
    "js": "./node_modules/webpack/bin/webpack.js --mode=production --display-error-details --bail",
    "js-dev": "./node_modules/webpack/bin/webpack.js --mode=development --display-error-details --bail",
    "sass": "./node_modules/node-sass/bin/node-sass scss/main.scss ../public/stylesheets/main.css",
    "compile": "npm run js && npm run sass",
    "dev": "npm run js-dev && npm run sass",
    "dev-server": "webpack-dev-server --port 9501 --public promo.thegulocal.com --disable-host-check",
    "devrun": "npm run dev-server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "angularjs-annotate"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "angular": "1.5.11",
    "angular-animate": "1.5.11",
    "angular-aria": "1.5.11",
    "angular-cookies": "1.5.11",
    "angular-i18n": "1.5.11",
    "angular-material": "1.0.8",
    "angular-messages": "1.5.11",
    "angular-mocks": "1.5.11",
    "angular-route": "1.5.11",
    "angular-sanitize": "1.5.11",
    "angular-ui-router": "0.3.0",
    "angular-uuid": "0.0.2",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.5",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "babel-polyfill": "6.23.0",
    "grunt": "1.0.1",
    "material-design-lite": "1.1.3",
    "ng-annotate-loader": "0.1.0",
    "node-sass": "4.5.0",
    "rimraf": "^3.0.0",
    "text-loader": "0.0.1",
    "webpack": "4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
